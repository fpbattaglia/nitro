                                <flag>wxGROW|wxALL</flag>
                                <border>0</border>
                                <option>1</option>
                                <object class="wxBoxSizer">
                                    <orient>wxHORIZONTAL</orient>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_TOP|wxALL</flag>
                                        <border>1</border>
                                        <object class="wxButton" name="ID_DEV_GET_BUTTON">
                                            <style>wxBU_EXACTFIT</style>
                                            <label>Get</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_TOP|wxALL</flag>
                                        <border>1</border>
                                        <object class="wxButton" name="ID_DEV_SET_BUTTON">
                                            <style>wxBU_EXACTFIT</style>
                                            <label>Set</label>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>
